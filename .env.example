# Environment variables for the automated job search system

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
LINKEDIN_EMAIL=your_linkedin_email_here
LINKEDIN_PASSWORD=your_linkedin_password_here

# Database settings
DATABASE_URL=sqlite:///jobs.db

# Web driver settings
WEB_DRIVER=chrome
HEADLESS=True

# File paths
RESUME_PATH=resumes/
COVER_LETTER_PATH=cover_letters/
OUTPUT_PATH=output/

# Application settings
DEFAULT_LOCATION=Remote
DEFAULT_RADIUS=50

# Skills analysis settings
SKILLS_THRESHOLD=0.7